{
  "manifest_version": 3,
  "name": "aichiver",
  "version": "<AUTO-GENERATED>",
  "description": "Archives your GF",
  "icons": {
    "48": "icons/logo_48.png",
    "96": "icons/logo_96.png"
  },
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "48": "icons/logo_48.png"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module",
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.chatgpt.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}